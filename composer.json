{
    "name": "pinkeen/api-debug-bundle",
    "description": "Allows easy API consumer debugging with web debug toolbar extension.",
    "keywords": ["symfony", "guzzle", "debug", "toolbar", "bundle", "api"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Filip Sobalski",
            "email": "pinkeen@gmail.com",
            "homepage": "http://sobalski.com/"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": ">=3.3",
        "symfony/symfony": ">=3.3",
        "psr/http-message": "~1.0",
        "guzzlehttp/psr7": "~1.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Install for easy API calling and integrated debugging."
    },
    "autoload":     {
        "psr-0": { "Pinkeen\\ApiDebugBundle": "" }
    },
    "target-dir": "Pinkeen/ApiDebugBundle"
}
